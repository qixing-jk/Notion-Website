{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=604800, stale-while-revalidate=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=604800"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=604800"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=604800"
        }
      ]
    },
    {
      "source": "/(\\w{2})?/404",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=3600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=86400"
        }
      ]
    },
    {
      "source": "/(\\w{2})?/tag/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=86400"
        }
      ]
    },
    {
      "source": "/(\\w{2})?/category/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=86400, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=86400"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=86400"
        }
      ]
    },
    {
      "source": "/(\\w{2})?/article/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    },
    {
      "source": "/(\\w{2})?/links",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    },
    {
      "source": "/_next/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=31536000, immutable"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=31536000"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=31536000"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    }
  ],
  "regions": [
    "hkg1"
  ]
}
