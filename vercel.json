{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=3600, stale-while-revalidate=600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=3600"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=3600"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=604800, stale-while-revalidate=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "max-age=604800"
        },
        {
          "key": "Vercel-CDN-Cache-Control",
          "value": "max-age=604800"
        },
        {
          "key": "Cloudflare-CDN-Cache-Control",
          "value": "max-age=604800"
        }
      ]
    },
